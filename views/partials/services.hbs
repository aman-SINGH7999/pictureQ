<section id="services-section" class="py-4">
        <div class="container">
            <h1 class="text-center text-secondary">Services That we Provied</h1>
            <div class="row mt-4">
                {{#each services}}
                <div class="col-md-4 my-3">
                    <div class="card">
                        <div class="card-body">
                            <div class="container text-center py-3"><i class="icon {{this.icon}}"></i></div>
                            <h5 class="card-title">{{this.title}}</h5>
                            <p>{{this.description}}</p>
                            <a href={{this.link_text}}>{{this.link}}</a>
                            <a href="#" class="ms-3">Support</a>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </section>